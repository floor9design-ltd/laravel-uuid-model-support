{
  "name": "floor9design/laravel-uuid-model-suport",
  "type": "library",
  "description": "A class to help laravel model offer simple UUID ID support",
  "keywords": [
    "floor9design",
    "laravel",
    "uuid"
  ],
  "homepage": "https://github.com/floor9design-ltd/laravel-casts-uuid",
  "license": "MIT",
  "authors": [
    {
      "name": "Rick Morice",
