      "homepage": "https://floor9design.com",
      "role": "Developer"
    }
  ],
  "require": {
    "laravel/framework": "^11|^10.0|^9.0",
    "php" : ">=8.2",
    "ramsey/uuid" : "^4"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.0.0",
    "phpunit/phpunit": "^11.0|^10.0|^9.0",
    "ramsey/uuid": "^4.2"
  },
  "autoload": {
    "psr-4": {
